{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "ignorePatterns": ["**/dist/*.js", "**/src/js/vendors/*.js"],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn",
    "linebreak-style": "off",
    "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0 }],
    "func-names": "off",
    "func-style": ["error", "declaration"],
    "no-var": "error"
  }
}
